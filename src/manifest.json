{
  "manifest_version": 2,
  "name": "RTL Confluence",
  "version": "0.9.0",
  "description": "Make confluence pages RTL",
  "icons": {
    "16": "rtl-icon-24x24.png",
    "128": "rtl-icon-240x240.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}